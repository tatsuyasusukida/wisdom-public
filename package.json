  "main": "index.js",
  "scripts": {
    "start": "node main.mjs",
    "dev": "nodemon -r dotenv/config main.mjs",
    "fixture": "node -r dotenv/config script/fixture.mjs",
    "deploy": "gcloud run deploy wisdom-public-staging --source . --region asia-northeast1 --platform managed --allow-unauthenticated",
